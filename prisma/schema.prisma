// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")

}

model UserAnswers {
  id      Int      @id @default(autoincrement())
  date    DateTime @db.DateTime()
  answers Json
}

model UserComments {
  id      Int      @id @default(autoincrement())
  date    DateTime @db.DateTime()
  answers String   
}